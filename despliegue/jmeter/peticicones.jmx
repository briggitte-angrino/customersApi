import java.text.SimpleDateFormat;


SimpleDateFormat t = new SimpleDateFormat(&quot;MMddhhmmss&quot;);
String transmissionDateTime = t.format(new Date());

JSONObject body = new JSONObject()

body.put(&quot;email&quot;,&quot;${correo}&quot;)
body.put(&quot;nombre&quot;, &quot;${nombre}&quot;)
body.put(&quot;apellido&quot;, &quot;${apellido}&quot;)
body.put(&quot;fecha&quot;,&quot;2023-01-03&quot;)
vars.put(&quot;body&quot;, body.toString())</stringProp>
